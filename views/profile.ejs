    <title><%=found.username%></title>
    <style>
        body{
            background-color: #F6F8FA;
        }
    </style>
</head>

<body>

    <%- include('component/navbar')  %>


    <div class="container-fluid profile-container">
        <div class="row justify-content-center">
            <div class="col-xl-3 col-lg-3 col-md-4 col-sm-11 col-11 profile p-3 mb-5 bg-white">
                <div class="intro">
                    <img src=<%=found.profilePic%> alt="profile">
                    <p class="text-center "><%= found.username %></p>
                    <p class="text-center username"><%= found.name %></p>
                    
                </div>
                <div class="details">
                        <div class="bio">
                                <p class="text-center"><%= found.bio %></p>
                            </div>
                    <div class="totalpost">
                        <span class="astrick">&#10033</span>
                        <p class="text-center"><strong><%= found.posts.length %></strong> Post Published </p>
                    </div>
                 
                    <div class="instagram">
                        <img src="/static/images/instagram.svg" width="35" height="35" alt="">
                       <% if(found.instagram === "username") {%>
                        <a href="#"><p></p></a>
                        <% }else{ %>
                            <a href=<%="https://instagram.com/"+found.instagram %>><p><%= found.instagram %></p></a>
                            <% } %>
                    </div>
                    <div class="twitter">
                        <img src="/static/images/twitter.svg" width="35" height="35" alt="">
                        <% if(found.twitter === "username") {%>
                            <a href="#"><p></p></a>
                            <% }else{ %>
                                <a href=<%="https://twitter.com/"+found.twitter %>><p><%= found.twitter %></p></a>
                                <% } %>
                    </div>
                    <div class="github">
                        <img src="/static/images/github.svg" width="35" height="35" alt="">
                        <% if(found.github === "username") {%>
                            <a href="#"><p></p></a>
                            <% }else{ %>
                                <a href=<%="https://github.com/"+found.github %>><p><%= found.github %></p></a>
                                <% } %>
                    </div>
                </div>

            </div>
            <div class="col-xl-5 col-lg-5 col-md-6 col-sm-10 col-10 mypostcontainer">
                <div class="row justify-content-center">

                    <% found.posts.reverse().forEach(post=>{ %>

                    <div class="col-lg-5 col-md-8 col-sm-12 col-12 mypost ">
                        <div class="p-3 bg-white">
                            
                            <div class="description">

                                <div class="date"><span><%= post.date %></span>
                                    <a href=<%="/tutorial/"+post.tech %> > <span class="tech badge badge-dark" id=<%=post.tech %>><%= post.tech %></span></a>
                                    </div>

                                <div class="title">
                                    <a href=<%=post.url%>><p><%= post.title %></p></a>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>

                    <div class="col-lg-12 col-md-12 col-sm-12 col-10 message">
                            <div>
                                <h5 class="text-center">No more posts found</h5>
                            </div>
                        </div>
                </div>
            </div>
        </div>
    </div>





    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->

